{
  "name": "mantrax",
  "version": "2.0.4",
  "description": "Core API for Mantra Phoenix: mantra-core with Mobx and support for Meteor 1.5+",
  "repository": {
    "type": "git",
    "url": "https://github.com/vladejs/mantrax.git"
  },
  "license": "MIT",
  "options": {
    "mocha": "--require scripts/mocha_runner src/**/__tests__/**/*.js"
  },
  "scripts": {
    "prepublish": ".\\scripts\\prepublish.bat",
    "lint": "eslint ./src",
    "lintfix": "eslint ./src --fix",
    "testonly": "mocha $npm_package_options_mocha",
    "test": "npm run lint && npm run testonly",
    "test-watch": "npm run testonly -- --watch --watch-extensions js"
  },
  "devDependencies": {
    "babel-cli": "^6.x.x",
    "babel-core": "^6.x.x",
    "babel-eslint": "latest",
    "babel-plugin-transform-runtime": "^6.x.x",
    "babel-polyfill": "^6.x.x",
    "babel-preset-es2015": "^6.x.x",
    "babel-preset-stage-2": "^6.x.x",
    "chai": "latest",
    "eslint": "latest",
    "mocha": "latest",
    "nodemon": "latest",
    "react": "latest"
  },
  "dependencies": {
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "6.x.x",
    "mobdux": "^1.0.4",
    "react-dom": "^15.6.1",
    "nqm-react-simple-di": "latest"
  }
}
